<div class="three-fourth main-text panel-content last">
	<span class="font-18px skin-font-color1 bold"><%= @title %></span>
	
	<%
	case @layout
	when 'mod' %>
		<%= render 'advertisments_mod', object: @advertisments %>
	<% when 'winner' %>
		<%= render 'advertisments_winner', object: @advertisments %>
	<% when 'ended' %>
		<%= render 'advertisments_ended', object: @advertisments %>
	<% else %>
		<%= render 'advertisments_active', object: @advertisments %>
	<% end %>

	<%= paginate @advertisments %>
	
